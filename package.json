{
  "name": "Cviceni-Eleventy-filmy",
  "version": "1.0.0",
  "description": "Postav projekt na Eleventy. Tentokrát začneš bez společného základu, který si budeš muset nachystat sama. Výsledek vystav na Netlify.",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch src/styly/index.scss dist/index.css",
    "build:sass": "sass src/styly/index.scss dist/index.css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JanaKaluzova/Cviceni-Eleventy-filmy.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JanaKaluzova/Cviceni-Eleventy-filmy/issues"
  },
  "homepage": "https://github.com/JanaKaluzova/Cviceni-Eleventy-filmy#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
